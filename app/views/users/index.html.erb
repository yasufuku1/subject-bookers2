<div class="container px-5 px-sm-0">
  <div class="row">
    <div class="col-md-3">
      <h2>User info</h2>
      <% @users.each do |user| %>
      <p><%= image_tag user.get_profile_image(100,100) %> </p>
      <table class="table">
        <tbody>
          <tr>
            <th>name</th>
            <th><%= user.name%></th>
          </tr>
          <tr>
            <th>introduction</th>
            <th><%= user.introduction %></th>
          </tr>
        </tbody>
        <% end %>
      </table>
      <div class="row">
        <%= link_to edit_user_path(@users.id), class: 'btn btn-outline-secondary btn-block' do %>
        <i class="fas fa-user-cog"></i>
        <% end %>
        <!--<a class="btn btn-outline-secondary btn-block edit_user_62194" href=user_path>-->
      </div>
    </div>
  </div>
</div>
